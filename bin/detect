#!/usr/bin/env node
# bin/detect <build-dir>

var fs = require('fs')

fs.statSync('package.json', function(err, stat) {
  if (err === null) {
    fs.readFileSync('VERSION', 'utf8', function(err, data){
      if (err === null) {
        console.log('node.js ' + data);
      }
    });
    process.exit(0);
  }
});

process.exit(1);